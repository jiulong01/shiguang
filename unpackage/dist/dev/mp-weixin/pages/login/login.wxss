	text-align: center;
	margin-bottom: 20rpx;
}
.login-form {
	margin-top: 10rpx;
}
.login-form input {
	width: 100%;
	padding: 20rpx;
	/* border: 1px solid #ccc; */
	/* border-radius: 4px; */
